#include <iostream>

using namespace std;
    
    unsigned long long factorial(int n) { 
        long long fac= 1;
        
        for (int i=2;i<=n;i++) { 
            fac *= i;
        }
        return fac;
    }
    
    unsigned long long mCn(int m, int n) {
        return factorial(m) / (factorial(n)*factorial(m-n));
    }
    
    
int main()
{
    int t, n, m;
    long long result;

    cin >> t;
    
    for(int h=t;h>0;h--) {
        
    cin >> n>> m;
    
    cout << mCn(m,n) << endl;
    
    }
    return 0;
}
